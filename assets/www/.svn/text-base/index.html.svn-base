    <script type="text/javascript" charset="utf-8" src="js/egovframework/mbl/hyb/NetworkAPI.js"></script>   
        
    <script type="text/javascript" charset="utf-8">
    
    	var egovHyb = new EgovHybrid("http://192.168.100.62:8080/DeviceAPIGuide_Web/");
    	
        
        // Wait for PhoneGap to load
        // 
        document.addEventListener("deviceready", onDeviceReady, false);
        
        // PhoneGap is loaded and it is now safe to make calls PhoneGap methods
        //
        function onDeviceReady() {  
            loadNetworkInfo();
            
        }
        
        
       
        
        //  iscroll setting
        var myScroll;
        function loadiScroll() {
        	document.addEventListener('touchmove', function (e) { e.preventDefault(); }, false);
            myScroll = new iScroll('wrapper');
        }
        
        

        
    </script>    
   
</head>
	
<body>

     
   

	<!-- Network Info Page -->
	<div id="networkInfo" data-role="page" data-theme="d">


		<div data-role="header" data-theme="d" data-position="fixed">
            <h1>네트워크 정보</h1>
            <a href="javascript:fn_goNetworkInfoList()" data-icon="arrow-r" data-theme="d" class="ui-btn-right">리스트</a>
		</div>



        <div data-role="content" >
            <div class="ui-grid-a">
                <div class="ui-block-a"><div class="ui-body-b" align="center">Platform</div></div>
                <div class="ui-block-b"><div class="ui-body-b" align="center">&nbsp;</div></div>
                <div class="ui-block-a"><div class="ui-body-b" align="center">Connection Type</div></div>
                <div class="ui-block-b"><div class="ui-body-b" align="center">&nbsp;</div></div>
                <div class="ui-block-a"><div class="ui-body-b" align="center">OS Version</div></div>
                <div class="ui-block-b"><div class="ui-body-b" align="center">&nbsp;</div></div>
            </div>
        </div>
        
		<div id="footer" data-role="footer" data-theme="a" data-position="fixed">
            <a href="javascript:confirm_registNetworkInfo()" data-icon="gear" data-theme="a" style="float: right; margin-right:10px;margin-top:8px;">정보 전송</a>
		</div>
	
	</div>

	<!-- Network Info List Saved In Server -->
    <div id="networkInfoList" data-role="page" data-theme="d">
        <div id="header" data-role="header" data-theme="d">
            <a href="javascript:$.mobile.changePage('#networkInfo', 'slide', false, false);" data-rel="back" data-role="button" data-icon="arrow-l" data-theme="d">뒤로</a>
            <h1>Network Info List</h1>
        </div>
        <div id="wrapper">
            <div id="scroller">
                <ul id="theList" data-role="listview" data-inset="true">
                    
                </ul>
            </div>
        </div>
        <div id="footer" data-role="footer" data-theme="a" data-position="fixed">
            <a href="javascript:confirm_deleteNetworkInfoList()" data-icon="gear" data-theme="a" style="float: right; margin-right:10px;margin-top:8px;">리스트 삭제</a>
        </div>
    </div>   
	

